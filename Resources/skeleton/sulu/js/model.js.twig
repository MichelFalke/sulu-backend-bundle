define(['mvc/relationalmodel'], function (relationalModel) {

    'use strict';

    return relationalModel({
        urlRoot: '/admin/api/{{ bundle_namespace }}/{{ entity_pluralize|snake_case('-') }}',

        defaults: function() {
            return {
            {% for field in metadata.fieldNames %}
            {{ field }}: null{% if not loop.last %},{% endif %}

            {% endfor %}
        };
        }
    });
});
